#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/git-raw-commits@5.0.0_conve_cc87af4a49a4e34375084da1ab8986bb/node_modules/git-raw-commits/src/node_modules:/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/git-raw-commits@5.0.0_conve_cc87af4a49a4e34375084da1ab8986bb/node_modules/git-raw-commits/node_modules:/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/git-raw-commits@5.0.0_conve_cc87af4a49a4e34375084da1ab8986bb/node_modules:/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/git-raw-commits@5.0.0_conve_cc87af4a49a4e34375084da1ab8986bb/node_modules/git-raw-commits/src/node_modules:/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/git-raw-commits@5.0.0_conve_cc87af4a49a4e34375084da1ab8986bb/node_modules/git-raw-commits/node_modules:/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/git-raw-commits@5.0.0_conve_cc87af4a49a4e34375084da1ab8986bb/node_modules:/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../git-raw-commits/src/cli.js" "$@"
else
  exec node  "$basedir/../git-raw-commits/src/cli.js" "$@"
fi
