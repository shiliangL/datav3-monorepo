#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/conventional-changelog-writer@8.0.1/node_modules/conventional-changelog-writer/dist/cli/node_modules:/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/conventional-changelog-writer@8.0.1/node_modules/conventional-changelog-writer/dist/node_modules:/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/conventional-changelog-writer@8.0.1/node_modules/conventional-changelog-writer/node_modules:/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/conventional-changelog-writer@8.0.1/node_modules:/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/conventional-changelog-writer@8.0.1/node_modules/conventional-changelog-writer/dist/cli/node_modules:/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/conventional-changelog-writer@8.0.1/node_modules/conventional-changelog-writer/dist/node_modules:/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/conventional-changelog-writer@8.0.1/node_modules/conventional-changelog-writer/node_modules:/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/conventional-changelog-writer@8.0.1/node_modules:/mnt/e/codeFreeStyle/datav3-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../conventional-changelog-writer@8.0.1/node_modules/conventional-changelog-writer/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../../../../../conventional-changelog-writer@8.0.1/node_modules/conventional-changelog-writer/dist/cli/index.js" "$@"
fi
